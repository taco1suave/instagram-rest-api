{
    "name": "valga/instagram-rest-api",
    "type": "library",
    "minimum-stability": "stable",
    "description": "A PHP wrapper for the Instagram REST and Search APIs",
    "keywords": [
        "Instagram",
        "REST",
        "API",
        "PHP"
    ],
    "authors": [
        {
            "name": "Abyr Valg",
            "email": "valga.github@abyrga.ru"
        }
    ],
    "autoload": {
        "psr-4": {
            "InstagramRestApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InstagramRestApi\\CodeStyle\\": "cs/"
        }
    },
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^6.2",
        "netresearch/jsonmapper": "^1.1",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "monolog/monolog": "^1.22",
        "friendsofphp/php-cs-fixer": "^2.3"
    }
}
